{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  baseBranches: ["main", "test"],
  vulnerabilityAlerts: {
    enabled: true,
  },
  "recreateWhen": "always",
  "packageRules": [
    {
      "managers": ["gomod"],
      "matchUpdateTypes": ["major", "minor"],
      "enabled": false
    },
  ]
}
